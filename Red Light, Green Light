#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,k;
	cin>>t;
	for(int i=0;i<t;i++){
	    int count=0;
	    cin>>n>>k;
	    int players[n];
	    for(int j=0;j<n;j++){
	        cin>>players[j];
	    }
	    
	    for(int j=0;j<n;j++){
	        if(players[j]>k){
	            count++;
	        }
	        
	    }
	    cout<<count<<endl;
	}
	return 0;
}
