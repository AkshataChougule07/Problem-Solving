#include <iostream>
using namespace std;

int main() {
	int t,n,c;
	cin>>t;
	for(int i=0;i<t;i++){
	    cin>>n;
	    
	    int s[n],d[n];
	    for(int i=0;i<n;i++){
	        cin>>s[i];
	    }
	   
	   for(int i=0;i<n;i++){
	        cin>>d[i];
	    }
	    c=0;
	    for(int i=0;i<n;i++){
	        if(s[i]==d[i]){
	            c++;
	        }
	    }
	    cout<<c<<endl;
	}
	return 0;
}
