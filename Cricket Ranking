#include<iostream>
using namespace std;
int main(){
	int t,r1,r2,w1,w2,c1,c2,j,k;
	cin>>t;
	for(int i=0;i<t;i++){
		cin>>r1>>w1>>c1;
		cin>>r2>>w2>>c2;
		
		int a1[]={r1,w1,c1};
		int a2[]={r2,w2,c2};
		int a=0,b=0;
		for(j=0;j<3;j++){
			if(a1[j]<a2[j]){
				b++;
			}
			else{
				a++;
			}
		}
		if(a>b){
			cout<<"A"<<endl;
		}
		else{
			cout<<"B"<<endl;
		}
	}
	return 0;
}
