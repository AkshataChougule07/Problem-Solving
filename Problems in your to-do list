#include <iostream>
using namespace std;

int main() {
	int t,n;
	cin>>t;
	for (int i=0;i<t;i++){
	    cin>>n;
	    int arr[n];
	    int c=0;
	    for(int j=0;j<n;j++){
	        cin>>arr[j];
	    }
	    for(int j=0;j<n;j++){
	        if(arr[j]>=1000){
	            c++;
	        }
	    } 
	    cout<<c<<endl;
	}
	return 0;
}
